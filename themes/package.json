  "scripts": {
    "lint": "stylelint \"**/*.scss\"",
    "watch": "sass sass_fodi/scss/style.scss:sass_fodi/css/style.css sass_fodi/scss/style-dark.scss:sass_fodi/css/style-dark.css --no-source-map --watch",
